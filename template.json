{
	"variables": {
		"build_timestamp": "{{timestamp}}"
	},
	"builders": [{
		"type": "virtualbox-iso",
		"boot_command": [
			"<esc><wait>",
			"<esc><wait>",
			"<enter><wait>",
			"/install/vmlinuz<wait>",
			" auto<wait>",
			" console-setup/ask_detect=false<wait>",
			" console-setup/layoutcode=us<wait>",
			" console-setup/modelcode=pc105<wait>",
			" debian-installer=en_US<wait>",
			" fb=false<wait>",
			" initrd=/install/initrd.gz<wait>",
			" kbd-chooser/method=us<wait>",
			" keyboard-configuration/layout=USA<wait>",
			" keyboard-configuration/variant=USA<wait>",
			" locale=en_US<wait>",
			" netcfg/get_hostname=ubuntu-1404<wait>",
			" netcfg/get_domain=vagrantup.com<wait>",
			" noapic<wait>",
			" preseed/url=http://{{ .HTTPIP }}:{{ .HTTPPort }}/preseed.cfg<wait>",
			" -- <wait>",
			"<enter><wait>"
		],
		"boot_wait": "10s",
		"disk_size": 40960,
		"guest_os_type": "Ubuntu_64",
		"http_directory": "http",
		"iso_checksum": "dd54dc8cfc2a655053d19813c2f9aa9f",
		"iso_checksum_type": "md5",
		"iso_url": "http://releases.ubuntu.com/trusty/ubuntu-14.04.5-server-amd64.iso",
		"output_directory": "output-virtualbox-iso-{{user `build_timestamp`}}",
		"ssh_username": "vagrant",
		"ssh_password": "vagrant",
		"ssh_port": 22,
		"ssh_wait_timeout": "10000s",
		"shutdown_command": "echo 'shutdown -P now' > /tmp/shutdown.sh; echo 'vagrant'|sudo -S sh '/tmp/shutdown.sh'",
		"vm_name": "packer-ubuntu-jenkins-master-{{user `build_timestamp`}}",
		"vboxmanage": [
			["modifyvm", "{{.Name}}", "--memory", "512"],
			["modifyvm", "{{.Name}}", "--cpus", "1"]
		]
	}, {
		"type": "vmware-iso",
		"boot_command": [
			"<esc><wait>",
			"<esc><wait>",
			"<enter><wait>",
			"/install/vmlinuz<wait>",
			" auto<wait>",
			" console-setup/ask_detect=false<wait>",
			" console-setup/layoutcode=us<wait>",
			" console-setup/modelcode=pc105<wait>",
			" debian-installer=en_US<wait>",
			" fb=false<wait>",
			" initrd=/install/initrd.gz<wait>",
			" kbd-chooser/method=us<wait>",
			" keyboard-configuration/layout=USA<wait>",
			" keyboard-configuration/variant=USA<wait>",
			" locale=en_US<wait>",
			" netcfg/get_hostname=ubuntu-1404<wait>",
			" netcfg/get_domain=vagrantup.com<wait>",
			" noapic<wait>",
			" preseed/url=http://{{ .HTTPIP }}:{{ .HTTPPort }}/preseed.cfg<wait>",
			" -- <wait>",
			"<enter><wait>"
		],
		"boot_wait": "10s",
		"disk_size": 40960,
		"guest_os_type": "linux",
		"headless": "true",
		"http_directory": "http",
		"iso_checksum": "dd54dc8cfc2a655053d19813c2f9aa9f",
		"iso_checksum_type": "md5",
		"iso_url": "http://releases.ubuntu.com/trusty/ubuntu-14.04.5-server-amd64.iso",
		"output_directory": "output-vmware-iso-{{user `build_timestamp`}}",
		"ssh_username": "vagrant",
		"ssh_password": "vagrant",
		"ssh_port": 22,
		"ssh_wait_timeout": "10000s",
		"shutdown_command": "echo 'shutdown -P now' > /tmp/shutdown.sh; echo 'vagrant'|sudo -S sh '/tmp/shutdown.sh'",
		"tools_upload_flavor": "linux",
		"vm_name": "packer-ubuntu-jenkins-master-{{user `build_timestamp`}}",
		"vmdk_name": "packer-ubuntu-jenkins-master-{{user `build_timestamp`}}",
		"vmx_data": {
			"memsize": "512",
			"numvcpus": "1",
			"cpuid.coresPerSocket": "1"
		}
	}],
	"provisioners": [{
		"type": "file",
		"source": "./files",
		"destination": "/tmp/files"
	}, {
		"type": "shell",
		"execute_command": "echo 'vagrant'|sudo -S sh '{{.Path}}'",
		"override": {
			"virtualbox-iso": {
				"scripts": [
					"scripts/base.sh",
					"scripts/vagrant.sh",
					"scripts/virtualbox.sh",
					"scripts/jenkins.sh",
					"scripts/cleanup.sh",
					"scripts/zerodisk.sh"
				]
			},
			"vmware-iso": {
				"scripts": [
					"scripts/base.sh",
					"scripts/vagrant.sh",
					"scripts/vmware.sh",
					"scripts/jenkins.sh",
					"scripts/cleanup.sh",
					"scripts/zerodisk.sh"
				]
			}
		}
	}],
	"post-processors": [{
		"type": "vagrant",
		"keep_input_artifact": true,
		"override": {
			"virtualbox": {
				"output": "packer-ubuntu-jenkins-master-virtualbox-{{user `build_timestamp`}}.box"
			},
			"vmware": {
				"output": "packer-ubuntu-jenkins-master-vmware-{{user `build_timestamp`}}.box"
			}
		}
	}]
}
